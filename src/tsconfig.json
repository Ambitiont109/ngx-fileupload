{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@r-hannuschka/ngx-fileupload": [
        "lib/public-api"
      ],
      "@utils/validators": [
        "example/utils/validators"
      ],
      "@ngx-fileupload-example/dashboard": [
        "example/libs/dashboard"
      ],
      "@ngx-fileupload-example/item-template": [
        "example/libs/item-template"
      ],
      "@ngx-fileupload-example/ui": [
        "example/libs/ui"
      ],
      "@ngx-fileupload-example/data": [
        "example/libs/data"
      ]
    }
  }
}
